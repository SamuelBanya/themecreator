{
	"tokenColors": [
		{
			"settings": {
				"background": "{{mainbg}}",
				"foreground": "{{mainfg}}"
			}
		},
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"foreground": "{{comment}}"
			}
		},
		{
			"name": "Foreground",
			"scope": [
				"keyword.operator.class",
				"constant.other",
				"source.php.embedded.line"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "{{mainfg}}"
			}
		},
		{
			"name": "Variable, String Link, Regular Expression, Tag Name, GitGutter deleted",
			"scope": [
				"variable",
				"support.other.variable",
				"string.other.link",
				"string.regexp",
				"entity.name.tag",
				"entity.other.attribute-name",
				"meta.tag",
				"declaration.tag",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "{{varible}}"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"variable.parameter",
				"punctuation.section.embedded",
				"keyword.other.unit"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "{{constant}}"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name.class",
				"entity.name.type.class",
				"support.type",
				"support.class"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#8c4a79"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading, GitGutter inserted",
			"scope": [
				"string",
				"constant.other.symbol",
				"entity.other.inherited-class",
				"entity.name.filename",
				"markup.heading",
				"markup.inserted.git_gutter"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "{{string}}"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.operator",
				"constant.other.color"
			],
			"settings": {
				"foreground": "{{keyword}}"
			}
		},
		{
			"name": "Function, Special Method, Block Level, GitGutter changed",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"support.function",
				"keyword.other.special-method",
				"meta.block-level",
				"markup.changed.git_gutter"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "{{functionname}}"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage",
				"storage.type"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "{{keyword}}"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"background": "{{varible}}",
				"fontStyle": "",
				"foreground": "{{mainfg}}"
			}
		},
		{
			"name": "Separator",
			"scope": "meta.separator",
			"settings": {
				"foreground": "{{mainfg}}"
			}
		},
		{
			"name": "Deprecated",
			"scope": "invalid.deprecated",
			"settings": {
				"background": "#1a8591",
				"fontStyle": "",
				"foreground": "{{mainfg}}"
			}
		},
		{
			"name": "Diff foreground",
			"scope": [
				"markup.inserted.diff",
				"markup.deleted.diff",
				"meta.diff.header.to-file",
				"meta.diff.header.from-file"
			],
			"settings": {
				"foreground": "{{mainfg}}"
			}
		},
		{
			"name": "Diff insertion",
			"scope": [
				"markup.inserted.diff",
				"meta.diff.header.to-file"
			],
			"settings": {
				"background": "{{string}}"
			}
		},
		{
			"name": "Diff deletion",
			"scope": [
				"markup.deleted.diff",
				"meta.diff.header.from-file"
			],
			"settings": {
				"background": "{{varible}}"
			}
		},
		{
			"name": "Diff header",
			"scope": [
				"meta.diff.header.from-file",
				"meta.diff.header.to-file"
			],
			"settings": {
				"foreground": "{{mainfg}}",
				"background": "{{functionname}}"
			}
		},
		{
			"name": "Diff range",
			"scope": "meta.diff.range",
			"settings": {
				"fontStyle": "italic",
				"foreground": "{{keyword}}"
			}
		}
	],
	"colors": {
		"editor.background": "{{mainbg}}",
		"editorCursor.foreground": "#cec5bd",
		"editor.foreground": "{{mainfg}}",
		"editorWhitespace.foreground": "#cec5bd",
		"editor.lineHighlightBackground": "#e1d8cf",
		"editor.selectionBackground": "#cec5bd"
	},
	"name": "white-sand"
}
